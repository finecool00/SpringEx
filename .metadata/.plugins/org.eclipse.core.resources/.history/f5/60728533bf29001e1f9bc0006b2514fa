  <!-- namespace에는 구현인터페이스 풀 경로로 적는다 -->
  <mapper namespace="com.simple.basic.mapper.TestMapper">
  	
  	<!-- id는 추상메서드 명이 들어갑니다!! -->
  	<!-- select문만의 특별한 하나인 resultType에는 결과를 반환받아서 나올 타입 or 객체가 들어감-->
  	<!-- 세미콜론은 적지 않습니다!! -->
  	<select id="getTime" resultType="string">
  		select now()
  	</select>
  	
  	<insert id="insertOne">
  	
  	</insert>
  
  </mapper>